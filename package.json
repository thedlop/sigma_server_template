{
  "name": "sigma_server_template",
  "version": "1.0.0",
  "description": "A simple ES6 setup for node applications wishing to use ergolib",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf dist && babel src --out-dir dist  --copy-files",
    "start": "node dist/app.js",
    "start:dev": "nodemon --experimental-modules --experimental-wasm-modules --exec babel-node src/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thedlop/sigma_server_template.git"
  },
  "keywords": [
    "babel",
    "node.js",
    "javascript",
    "es6"
  ],
  "author": "Dark Lord of Programming",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thedlop/sigma_server_template/issues"
  },
  "homepage": "https://github.com/thedlop/sigma_server_template#readme",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/node": "^7.16.5",
    "@babel/plugin-proposal-class-properties": "^7.16.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "@coinbarn/ergo-ts": "^0.3.0",
    "ergo-lib-wasm-browser": "^0.15.0",
    "ergo-lib-wasm-nodejs": "^0.15.0",
    "express": "^4.17.2"
  }
}
